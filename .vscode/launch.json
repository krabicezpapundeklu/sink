    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Launch Server",
            "cargo": {
                "args": [
                    "build"
                ]
            },
            "args": ["start-server"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Node",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/web/build/index.js"
        }
    ]
}
